    "clipboard_read": false,
    "clipboard_write": true,
    "hotkey_register": true,
    "camera_capture": null,
    "microphone_record": null,
    "webcam_capture": {
      "mode": "user_initiated_only"
    },
    "health_read": null,
    "health_export": false
  },
  "risk_level": "Sensitive",
