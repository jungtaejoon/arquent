    "camera_capture": null,
    "microphone_record": null,
    "webcam_capture": null,
    "health_read": {
      "types": ["steps_daily"],
      "aggregation": "daily_summary"
    },
    "health_export": false
  },
  "risk_level": "Sensitive",
  "user_initiated_required": true,
  "signature": null,
  "publisher": null
}
